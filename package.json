{
  "name": "RSS",
  "version": "1.0.0",
  "description": "RSS reader on ReactJS",
  "main": "./src/index.js",
  "scripts": {
    "test": "better-npm-run test",
    "test:watch": "npm run test -- --watch",
    "start": "babel-node ./server/prod.js",
    "dev": "better-npm-run dev",
    "devmon": "nodemon --exec better-npm-run dev",
    "build": "better-npm-run build",
    "dll:dev": "better-npm-run dll:dev",
    "dll:prod": "better-npm-run dll:prod",
    "dll": "npm run dll:dev && npm run dll:prod",
    "postinstall": "npm run dll",
    "postuninstall": "npm run dll",
    "webpack": "babel-node node_modules/webpack/bin/webpack.js"
  },
  "betterScripts": {
    "dev": {
      "command": "babel-node ./server/dev.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "npm run webpack -- --optimize-minimize --color --progress --config=webpack/webpack.config.prod.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "jest",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "dll:prod": {
      "command": "npm run webpack -- --color --progress --config=webpack/webpack.config.dll.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "dll:dev": {
      "command": "npm run webpack -- --color --progress --config=webpack/webpack.config.dll.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Apination/workflow-builder.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Apination/workflow-builder.git/issues"
  },
  "homepage": "https://github.com/Apination/workflow-builder.git#readme",
  "dependencies": {
    "body-parser": "^1.17.1",
    "classnames": "^2.2.5",
    "date-fns": "^1.28.3",
    "express": "^4.15.2",
    "googleapis": "^19.0.0",
    "idx": "^1.5.0",
    "jsonwebtoken": "^7.3.0",
    "lodash": "^4.17.4",
    "mongoose": "^4.9.4",
    "morgan": "^1.8.1",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "prop-types": "^15.5.6",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-gemini-scrollbar": "^2.3.0",
    "react-helmet": "^5.0.2",
    "react-redux": "^5.0.2",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.1",
    "redux-saga": "^0.15.0",
    "reselect": "^3.0.0",
    "rss-parser": "^2.6.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.0",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "better-npm-run": "^0.0.15",
    "bootstrap": "^3.3.7",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "css-mqpacker": "^5.0.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.14.1",
    "eslint-config-standard": "^10.0.0",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "img-loader": "^2.0.0",
    "jest": "^19.0.2",
    "merge-deep": "^3.0.0",
    "mock-express-response": "^0.2.0",
    "node-sass": "^4.3.0",
    "opener": "^1.4.2",
    "postcss-loader": "^1.2.2",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.4.2",
    "redux-logger": "^3.0.1",
    "sass-loader": "^6.0.1",
    "style-loader": "^0.16.1",
    "svg-loader": "0.0.2",
    "svg-url-loader": "^2.0.2",
    "svgo": "^0.7.2",
    "svgo-loader": "^1.1.2",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1",
    "webpack-node-externals": "^1.5.4",
    "why-did-you-update": "^0.0.8"
  },
  "jest": {
    "testPathDirs": [
      "./src",
      "./server"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "./src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/fileMock.js",
      "\\.(css|scss|sass)$": "<rootDir>/tests/styleMock.js"
    }
  }
}
